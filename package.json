{
  "name": "@fretstop/root",
  "version": "1.0.0",
  "private": true,

  "description": "The Fret Stop Commmunity Application",
  "scripts": {
    "install:all": "yarn install:client && yarn install:server",
    "build:all": "yarn build:client && yarn build:server",
    "install:client": "cd client && yarn install",
